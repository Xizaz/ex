
#include <unistd.h>

int		ft_verif_char(char *str, char c, int index)
{
	int		i;												//

	i = 0;
	while (i < index)										// lettre de string 1,i est deja string 1 -> 0
	{														// 		 							   else	   -> 1
		if (str[i] == c)
			return (0);
		i++;
	}
	return (1);
}

void	ft_inter(char *s1, char *s2)
{
	int		i;
	int		j;

	i = 0;
	while (s1[i])											//string 1,i existe
	{
		if (ft_verif_char(s1, s1[i], i) == 1)				//si pas de doublon
		{
			j = 0;
			while (s2[j])									//si string 2 existe
			{
				if (s2[j] == s1[i])							//si string 2,j = string 1,i ecrire string 2,j et sortir du while
				{
					write(1, &s1[i], 1);							
					break ;
				}
				j++;
			}
		}
		i++;
	}
}

int		main(int argc, char **argv)
{
	if (argc == 3)
		ft_inter(argv[1], argv[2]);
	write(1, "\n", 1);
	return (0);
}